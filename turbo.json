{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				"dist/**",
				"build/**"
			]
		},
		"db:make-migration": {
			"cache": false,
			"persistent": true
		},
		"db:migrate": {
			"cache": false,
			"persistent": true
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"type-check": {
			"dependsOn": [
				"^type-check"
			]
		},
		"lint": {},
		"test": {
			"dependsOn": [
				"^build"
			]
		}
	}
}